{
  "permissions": {
    "allow": [
      "Bash(go mod:*)",
      "Bash(curl:*)",
      "Bash(mkdir:*)",
      "Bash(tar:*)",
      "Bash(export PATH=$HOME/go_local/go/bin:$PATH)",
      "Bash(export GOPATH=$HOME/go)",
      "Bash(go:*)",
      "Bash(docker-compose up:*)",
      "Bash(docker compose:*)",
      "WebFetch(domain:golangci-lint.run)",
      "Bash(rm:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(ls)",
      "Bash(mv:*)",
      "WebFetch(domain:pkg.go.dev)",
      "Bash(git checkout:*)",
      "Bash(npx create-next-app:*)",
      "Bash(npm install:*)",
      "Bash(npm cache clean:*)",
      "Bash(npx:*)",
      "Bash(npm run lint)",
      "Bash(npm run type-check:*)",
      "Bash(touch:*)"
    ],
    "deny": []
  },
  "env": {
    "GOPATH": "$HOME/go",
    "PATH": "$HOME/go_local/go/bin:$PATH",
    "BASH_DEFAULT_TIMEOUT_MS": "600000",
    "BASH_MAX_TIMEOUT_MS": "1200000"
  }
}
